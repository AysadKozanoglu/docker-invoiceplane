INVOICEPLANE_FQDN=${INVOICEPLANE_FQDN:-localhost}

INVOICEPLANE_TIMEZONE=${INVOICE_PLANE_TIMEZONE:-} # backward compatibility
INVOICEPLANE_TIMEZONE=${INVOICEPLANE_TIMEZONE:-UTC}

## DATABASE
DB_HOST=${DB_HOST:-}
DB_PORT=${DB_PORT:-}
DB_NAME=${DB_NAME:-}
